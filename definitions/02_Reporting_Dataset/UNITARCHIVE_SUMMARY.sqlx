config {
  schema: "ReportingDataset",
  type: "view",
  materialized: true,  
  
  tags: ["Reporting Table History Data"]
}
SELECT
  xx.DUID,
  yy.StationName,
  yy.Region,
  yy.FuelSourceDescriptor,
  yy.Technology,
  yy.latitude,
  yy.longitude,
  yy.Tech,
  DAY,
  SUM(Mwh) AS Mwh
FROM
  ${ref("UNITARCHIVE")} xx
INNER JOIN
  test-187010.aemodataset.DUID yy
ON
  xx.DUID=YY.DUID
GROUP BY
  1,
  2,
  3,
  4,
  5,
  6,
  7,
  8,
  9
